  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListName>Auflauf von saurer Sahne</ns0:igdtListName>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>1 Ma&#223;</ns0:igdtQuantity>
        <ns0:igdtName ref="sahne">saure Sahne</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>8</ns0:igdtQuantity>
