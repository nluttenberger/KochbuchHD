  <ns0:recipeName>Herzogs-Auflauf</ns0:recipeName>
  <ns0:recipeKeywords />
  <ns0:recipeIntro />
  <ns0:recipeIngredients>
    <ns0:igdtList>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>8</ns0:igdtQuantity>
        <ns0:igdtName ref="ei">Eidottern</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>&#188; Ma&#223;</ns0:igdtQuantity>
        <ns0:igdtName ref="sahne">Sahne</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>&#188; Pfund</ns0:igdtQuantity>
        <ns0:igdtName ref="weizenmehl">Mehl</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>6 Loth</ns0:igdtQuantity>
        <ns0:igdtName ref="zucker">Zucker</ns0:igdtName>
      </ns0:igdtListLine>
      <ns0:igdtListLine>
        <ns0:igdtQuantity>6 Loth</ns0:igdtQuantity>
